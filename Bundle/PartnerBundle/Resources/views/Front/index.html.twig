{% extends "AvantagesPartnerBundle::basepartner.html.twig" %}
{% block body %}
<div id="myCarousel" class="carousel slide">
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <!-- Carousel items -->
  <div class="carousel-inner" style="max-width:1100px;margin: 0 auto;float:none">
    <div class="active item"><img src="{{ asset('img/1.jpg') }}"/></div>
    <div class="item"><img src="{{ asset('img/2.jpg') }}"/></div>
    <div class="item"><img src="{{ asset('img/3.jpg') }}"/></div>
  </div>
  <!-- Carousel nav -->
  <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
  <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
</div>
{% endblock %}